[core]
        excludesfile = .gitignore
[user]
	email = mschmitt@db12.de
	name = Matthias Schmitt
[color]
	status = true
	branch = true
	diff = true
[alias]
    sta = stash
    stap = stash apply
    cm = commit -m
    cp = cherry-pick
    sr = svn rebase
    sdc = svn dcommit
    flfs = flow feature start
    flff = flow feature finish
    flrs = flow release start
    flrf = flow release finish
    st = status
    br = branch
    co = checkout
    df = diff
    lg = log -p
	reco = rebase --continue
	resk = rebase --skip